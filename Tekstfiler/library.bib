keywords = {Binding, Competitive,Blotting, Northern,Carcinoma,Carcinoma: metabolism,Carcinoma: pathology,Fibroblast Growth Factor 2,Fibroblast Growth Factor 2: metabolism,Humans,Receptors, Cell Surface,Receptors, Cell Surface: metabolism,Tumor Cells, Cultured,Urinary Bladder Neoplasms,Urinary Bladder Neoplasms: metabolism,Urinary Bladder Neoplasms: pathology},
