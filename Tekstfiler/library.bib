file = {:Users/jqc305/Library/Mobile Documents/3L68KQB4HG{\~{}}com{\~{}}readdle{\~{}}CommonDocuments/Documents/Artikler/Histopathological survey of neoplasms in flat-coated retrievers, 1990 to 1998. - Morris et al. - 2000.pdf:pdf},
issn = {0042-4900},
journal = {The Veterinary record},
month = {sep},
number = {11},
