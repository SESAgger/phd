keywords = {Dog,Drug sensitivity,Ivermectin,MDR1,P-glycoprotein},
pages = {67--71},
title = {{Breed distribution of the nt230(del4) MDR1 mutation in dogs}},
url = {https://ac-els-cdn-com.ep.fjernadgang.kb.dk/S1090023310002261/1-s2.0-S1090023310002261-main.pdf?{\_}tid=7b4ab9e0-d778-4252-a936-6fbd1bbd1e10{\&}acdnat=1552916738{\_}f9d041dd020c359a184598cb247fbf87},
