journal = {Brenner's Encyclopedia of Genetics: Second Edition},
keywords = {Evolution,F-statistics,Fitness,Genotype,Heterozygosity,Mutation,Parameters,Phenotype,Selection,Variance},
pages = {290--291},
title = {{Genetic Variation}},
