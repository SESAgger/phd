isbn = {0916-7250 (Print)$\backslash$r0916-7250 (Linking)},
issn = {0916-7250},
journal = {The Journal of veterinary medical science / the Japanese Society of Veterinary Science},
keywords = {2007,271,278,3,69,canine hemangiosarcoma,cell growth and division,competing growth inhibitors and,cyclin d1,involve complicated pathways,j,med,p16,p53,pro-,rb,sci,that are governed by,vet},
