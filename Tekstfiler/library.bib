file = {:Users/jqc305/Library/Mobile Documents/3L68KQB4HG{\~{}}com{\~{}}readdle{\~{}}CommonDocuments/Documents/Artikler/Responding to devious demands for co-authorship A rejoinder to B{\"{u}}low and Helgesson's ‘dirty hands' justification - Tang - 2018.pdf:pdf},
issn = {1747-0161},
journal = {Research Ethics},
keywords = {Academic publishing,authorship criteria,dirty hands,hostage authorship,research ethics},
