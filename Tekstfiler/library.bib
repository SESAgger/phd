title = {{What is the DNA repair defect underlying Fanconi anemia?}},
url = {https://www-sciencedirect-com.ep.fjernadgang.kb.dk/science/article/pii/S095506741500112X?via{\%}3Dihub http://dx.doi.org/10.1016/j.ceb.2015.09.002},
volume = {37},
