@article{Dobson2013a,
abstract = {Cancer is a common problem in dogs and although all breeds of dog and crossbred dogs may be affected, it is notable that some breeds of pedigree dogs appear to be at increased risk of certain types of cancer suggesting underlying genetic predisposition to cancer susceptibility. Although the aetiology of most cancers is likely to be multifactorial, the limited genetic diversity seen in purebred dogs facilitates genetic linkage or association studies on relatively small populations as compared to humans, and by using newly developed resources, genome-wide association studies in dog breeds are proving to be a powerful tool for unravelling complex disorders. This paper will review the literature on canine breed susceptibility to histiocytic sarcoma, osteosarcoma, haemangiosarcoma, mast cell tumours, lymphoma, melanoma, and mammary tumours including the recent advances in knowledge through molecular genetic, cytogenetic, and genome wide association studies.},
