file = {:Users/jqc305/Library/Mobile Documents/3L68KQB4HG{\~{}}com{\~{}}readdle{\~{}}CommonDocuments/Documents/Artikler/Interleukin-8 promotes canine hemangiosarcoma growth by regulating the tumor microenvironment - Kim et al. - 2014.pdf:pdf},
