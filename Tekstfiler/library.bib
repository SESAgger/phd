isbn = {3617496768},
issn = {0888-8809},
journal = {Molecular Endocrinology},
number = {7},
pages = {1519--1534},
pmid = {16484338},
title = {{Progesterone Signaling in Human Myometrium through Two Novel Membrane G Protein-Coupled Receptors: Potential Role in Functional Progesterone Withdrawal at Term}},
url = {https://academic.oup.com/mend/article-lookup/doi/10.1210/me.2005-0243},
