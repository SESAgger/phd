doi = {10.1111/vco.12258},
file = {:Users/jqc305/Library/Mobile Documents/3L68KQB4HG{\~{}}com{\~{}}readdle{\~{}}CommonDocuments/Documents/Artikler/Differences in the geographic distribution of lymphoma subtypes in Golden retrievers in the USA - Ruple, Avery, Morley - 2016.pdf:pdf},
issn = {14765829},
