file = {:Users/jqc305/Library/Mobile Documents/3L68KQB4HG{\~{}}com{\~{}}readdle{\~{}}CommonDocuments/Documents/Artikler/PI3KAkt signalling pathway and cancer - Vara et al. - 2004.pdf:pdf},
