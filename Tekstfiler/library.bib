number = {1},
pages = {155--164},
pmid = {24582862},
publisher = {Elsevier},
title = {{Interleukin-8 promotes canine hemangiosarcoma growth by regulating the tumor microenvironment}},
url = {http://dx.doi.org/10.1016/j.yexcr.2014.02.020},
