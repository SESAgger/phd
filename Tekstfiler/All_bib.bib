month = {jul},
number = {2},
pages = {75--86},
publisher = {Elsevier},
title = {{Heritability and epidemiology of canine hip-dysplasia score in flat-coated retrievers and Newfoundlands in the United Kingdom}},
url = {https://www-sciencedirect-com.ep.fjernadgang.kb.dk/science/article/pii/S0167587700001409},
volume = {46},
year = {2000}
}
