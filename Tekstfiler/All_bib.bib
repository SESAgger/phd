issn = {03009858},
journal = {Veterinary pathology},
keywords = {and phosphotungstic acid,dog,hematoxylin,immunohistochemistry,malignant fi brous histiocytoma,masson trichrome,ptah,reticulin,sarcoma,son,the immunohistochemical antibodies},
number = {4},
pages = {473--479},
title = {{Immunohistochemical and histopathologic features of 14 malignant fibrous histiocytomas from Flat-Coated Retrievers.}},
volume = {39},
year = {2002}
