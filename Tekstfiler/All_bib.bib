publisher = {Elsevier B.V.},
title = {{Cell-free DNA: Preanalytical variables}},
url = {https://www-sciencedirect-com.ep.fjernadgang.kb.dk/science/article/pii/S0009898115004088 http://dx.doi.org/10.1016/j.cca.2015.08.028},
volume = {450},
