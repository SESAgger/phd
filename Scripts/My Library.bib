@article{diosAneurysmalBoneCyst1992,
  title = {Aneurysmal Bone Cyst. {{A}} Clinicopathologic Study of 238 Cases},
  author = {Dios, Ariel M. Vergel De and Bond, Jeffrey R. and Shives, Thomas C. and McLeod, Richard A. and Unni, K. Krishnan},
