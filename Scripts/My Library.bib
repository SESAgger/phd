@article{martinezAneurysmalBoneCyst1988,
  title = {Aneurysmal Bone Cyst. {{A}} Review of 123 Cases Including Primary Lesions and Those Secondary to Other Bone Pathology},
  author = {Martinez, Virginia and Sissons, Hubert A.},
  year = {1988},
  volume = {61},
